<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>E-commerceGitHub</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>products-edit.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Client.Register.Essentials</servlet-name>
    <servlet-class>Client.Register.Essentials</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Client.Register.Essentials</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Client.LoginOut.Login</servlet-name>
    <servlet-class>Client.LoginOut.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Client.LoginOut.Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Client.LoginOut.Logout</servlet-name>
    <servlet-class>Client.LoginOut.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Client.LoginOut.Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Admin.Products.total</servlet-name>
    <servlet-class>Admin.Products.total</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Admin.Products.total</servlet-name>
    <url-pattern>/total</url-pattern>
  </servlet-mapping>
</web-app>